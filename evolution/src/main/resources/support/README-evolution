cp $DEPLOY_ROOT/support/subscribergroup-* <_NGLM_SUBSCRIBERGROUP_DATA_>
$DEPLOY_ROOT/bin/update-subscribergroup.sh new segmentationDimension churnRisk subscribergroup-churnrisk.001
$DEPLOY_ROOT/bin/update-subscribergroup.sh add segmentationDimension churnRisk subscribergroup-churnrisk.002
$DEPLOY_ROOT/bin/update-subscribergroup.sh remove segmentationDimension churnRisk subscribergroup-churnrisk.002
$DEPLOY_ROOT/bin/update-subscribergroup.sh delete segmentationDimension churnRisk

